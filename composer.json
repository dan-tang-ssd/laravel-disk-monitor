{
    "name": "spatie/laravel-disk-monitor",
    "description": "Monitor metrics of Laravel disks",
    "keywords": [
        "Spatie",
        "laravel",
        "laravel-disk-monitor"
    ],
    "homepage": "https://github.com/spatie/laravel-disk-monitor",
    "license": "MIT",
    "authors": [
        {
            "name": "Dan Tang",
            "email": "dan@stats4sd.org",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.37",
        "illuminate/console": "^6.0|^7.0",
        "illuminate/database": "^6.0|^7.0",
        "illuminate/http": "^6.0|^7.0",
        "illuminate/support": "^6.0|^7.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^4.0|^5.0|^6.15",
        "pestphp/pest": "^1.18",
        "pestphp/pest-plugin-laravel": "^1.1",
        "spatie/laravel-ray": "^1.23",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\LaravelDiskMonitor\\": "src",
            "Spatie\\LaravelDiskMonitor\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\LaravelDiskMonitor\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/pest --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\LaravelDiskMonitor\\LaravelDiskMonitorServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
